<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">

	<NameField>Chert</NameField>



	<DialogueNode>
		<Name>Questions</Name>
			
		<DialogueOptionsList>
			<DialogueOption>
					<Text>Would you like to roast one last marshmallow together, Chert?</Text>
					<DialogueTarget>Chert SW1</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>Chert SW1</Name>
			<Dialogue>
				<Page>No.. No, thank you.</Page>
				<Page>But..</Page>
				<Page>When the sun explodes, could you leave a marshmallow on the stick for me?</Page>	
				<Page>I'd like to see how quickly it burns up.. For personal reference.</Page>
			</Dialogue>
	</DialogueNode>




</DialogueTree>
